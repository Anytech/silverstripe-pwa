{
    "name": "Anytech/silverstripe-pwa",
    "description": "Module that adds PWA support to SilverStripe project",
    "homepage": "http://github.com/mdiederen/silverstripe-pwa",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "progressive",
        "web",
        "app",
        "PWA",
        "Push",
        "notifications",
        "service worker",
        "manifest"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Kayne Middleton",
        "email": "kayne@anytech.ca"
    }],
    "support": {
        "issues": "http://github.com/Anytech/silverstripe-pwa/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/cms": "^4.0@dev",
        "silverstripe/framework": "^4",
        "silverstripe/vendor-plugin": "^1.0",
        "silverware/colorpicker": "^1.0",
        "minishlink/web-push": "^5.2"
        },
    "autoload": {
        "psr-4": {
            "anytech\\silverstripe-pwa\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "installer-name": "silverstripe-pwa",
        "expose": [
            "resources"
        ]
    }
}
