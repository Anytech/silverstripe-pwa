{
    "name": "mdiederen/silverstrip-pwa",
    "description": "Module that adds PWA support to SilverStripe project",
    "homepage": "http://github.com/mdiederen/silverstrip-pwa",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "progressive",
        "web",
        "app",
        "PWA"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Michiel Diederen",
        "email": "michiel@violet88.nl"
    }],
    "support": {
        "issues": "http://github.com/mdiederen/silverstrip-pwa/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/cms": "^4.0@dev",
        "silverstripe/framework": "^4",
        "silverstripe/vendor-plugin": "^1.0",
        "silverware/colorpicker": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "mdiederen\\silverstrip-pwa\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "installer-name": "silverstrip-pwa",
        "expose": [
            "resources"
        ]
    }
}
